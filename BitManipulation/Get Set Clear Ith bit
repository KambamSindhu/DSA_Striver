// User function Template for Java

class Solution {
    static void bitManipulation(int num, int i) {
        // code here
         int getI = (num & (1 << (i - 1))) != 0 ? 1 : 0;

        // Set i-th bit
        int setI = num | (1 << (i - 1));

        // Clear i-th bit
        int clearI = num & ~(1 << (i - 1));

        System.out.print(getI + " " + setI + " " + clearI);
    }
}
