// User function Template for Java

class Solution {
    public int binaryToDecimal(String b) {
        // Code here
        int res=0;
        int p2=1;
        
        for(int i=b.length()-1;i>=0;i--)
        {
            if(b.charAt(i)=='1')
            {
                res+=p2;
            }
            p2=p2*2;
        }
        return res;
    }
}
