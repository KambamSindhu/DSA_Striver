// User function Template for Java

class Solution {
    public int maximumProfit(int arr[]) {
        // Code here
          // code here
        int min=arr[0];
        int profit=0;
        for(int i=1;i<arr.length;i++)
        {
            int cost=arr[i]-min;
            min=Math.min(min,arr[i]);
            profit=Math.max(cost,profit);
        }
        return profit;
    }
}
