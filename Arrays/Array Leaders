class Solution {
    static ArrayList<Integer> leaders(int arr[]) {
        // code here
    ArrayList<Integer> list=new ArrayList<Integer>();
    int n=arr.length;
     int max=arr[n-1];
    list.add(max);
     for(int i=n-2;i>=0;i--)
     {
         if(arr[i]>=max)
         {
                 list.add(0, arr[i]);
             max=Math.max(arr[i],max);
         }
     }
     return list;
    }
}
